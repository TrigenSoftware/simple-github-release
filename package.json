{
  "name": "simple-github-release",
  "type": "module",
  "version": "0.0.1-1",
  "description": "Simple tool to create GitHub releases.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrigenSoftware/simple-github-release.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/simple-github-release/issues"
  },
  "main": "./src/index.ts",
  "bin": "./src/cli/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "bin": "./dist/cli/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      "default": "./dist/index.js"
    },
    "directory": "package"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prepublishOnly": "pnpm test && pnpm build && del ./package && clean-publish",
    "postpublish": "del ./package",
    "lint": "trigen-scripts lint",
    "jest": "trigen-scripts jest",
    "test": "trigen-scripts test",
    "test:size": "size-limit",
    "build": "trigen-scripts build",
    "clean": "trigen-scripts clean",
    "commit": "cz",
    "release": "standard-version",
    "updateGitHooks": "simple-git-hooks"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "@types/git-url-parse": "^9.0.1",
    "argue-cli": "^2.0.0-2",
    "git-url-parse": "^11.6.0",
    "lilconfig": "^2.0.4",
    "new-github-release-url": "^2.0.0",
    "open": "^8.4.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@commitlint/cz-commitlint": "^15.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@size-limit/file": "^7.0.5",
    "@swc/core": "^1.2.119",
    "@swc/jest": "^0.2.14",
    "@trigen/browserslist-config": "^8.0.0-alpha.2",
    "@trigen/eslint-config": "^8.0.0-alpha.3",
    "@trigen/scripts": "^8.0.0-alpha.2",
    "@trigen/scripts-plugin-eslint": "^8.0.0-alpha.3",
    "@trigen/scripts-plugin-jest": "^8.0.0-alpha.2",
    "@trigen/scripts-plugin-swc": "^8.0.0-alpha.2",
    "@trigen/scripts-plugin-typescript": "^8.0.0-alpha.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "browserslist": "^4.19.1",
    "clean-publish": "^3.4.0",
    "commitizen": "^4.2.4",
    "del-cli": "^4.0.1",
    "eslint": "^8.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.5",
    "nano-staged": "^0.5.0",
    "rollup": "^2.64.0",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-swc": "^0.2.0",
    "simple-git-hooks": "^2.7.0",
    "size-limit": "^7.0.5",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist"
  ],
  "readme": ""
}
